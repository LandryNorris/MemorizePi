CREATE TABLE Round (
    id INTEGER NOT NULL PRIMARY KEY,
    score INTEGER AS Int NOT NULL,
    timestamp INTEGER NOT NULL,
    secondsInGame INTEGER NOT NULL
);

selectAll:
SELECT * FROM Round;

selectAllScores:
SELECT score FROM Round;

selectTopScores:
SELECT score FROM Round LIMIT ?;

insert:
INSERT INTO Round(score, timestamp, secondsInGame) VALUES (?, ?, ?);

